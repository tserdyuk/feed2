{
  "version": "2.0",
  "start": "jade:html -> watch:html, scriptWatch:app -> serve",
  "serve": ["cd ionic; ionic serve"],
  ":html": {
    "task": "jade:index",
    "plumber": true,
    "pretty": true,
    "src": "src/**/*.jade",
    "dest": "ionic/www"
  },
  ":app": {
    "main": "src/app.js",
    "output": "app.js",
    "dest": "ionic/www"
  }
}
