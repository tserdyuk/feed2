{
  "version": "2.0",
  "start": "clean -> jade:html -> watch:html, scriptWatch:app -> serve",
  "serve": ["cd ionic; ionic serve"],
  ":html": {
    "task": "jade:html",
    "plumber": true,
    "pretty": true,
    "src": "src/**/*.jade",
    "dest": "ionic/www"
  },
  ":app": {
    "main": "src/js/app.js",
    "output": "app.js",
    "dest": "ionic/www"
  },
  "clean": {
    "dest": "ionic/www/views"
  }
}
